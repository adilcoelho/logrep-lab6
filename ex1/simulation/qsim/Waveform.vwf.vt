module ex1_vlg_vec_tst();
// constants                                           
// general purpose registers
reg clk;
reg dec;
reg inc;
// wires                                               
wire [31:0] debug_out;
wire [27:0] ssd_saida;

wire sampler;                             

// assign statements (if any)                          
ex1 i1 (
// port map - connection between master ports and signals/registers   
	.clk(clk),
	.debug_out(debug_out),
	.dec(dec),
	.inc(inc),
	.ssd_saida(ssd_saida)
);

// clk
always
begin
	clk = 1'b0;
	clk = #1250 1'b1;
	#1250;
end 

// dec
initial
begin
	dec = 1'b1;
	dec = #305000 1'b0;
	# 5000;
