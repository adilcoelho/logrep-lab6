	begin
		dec = 1'b0;
		dec = #5000 1'b1;
		# 5000;
	end
	dec = 1'b0;
	dec = #5000 1'b1;
	dec = #35000 1'b0;
	dec = #5000 1'b1;
	dec = #5000 1'b0;
	dec = #5000 1'b1;
	dec = #125000 1'b0;
	dec = #5000 1'b1;
	# 5000;
	repeat(8)
	begin
		dec = 1'b0;
		dec = #5000 1'b1;
		# 5000;
	end
	dec = 1'b0;
	dec = #5000 1'b1;
	# 95000;
	repeat(4)
	begin
		dec = 1'b0;
		dec = #5000 1'b1;
		# 5000;
	end
end 

// inc
initial
begin
	inc = 1'b1;
	inc = #5000 1'b0;
	# 5000;
	repeat(29)
	begin
		inc = 1'b1;
		inc = #5000 1'b0;
